{% extends 'base.html.twig' %}

{% block title %}ApiRadio index
{% endblock %}

{% block body %}
	<h1>ApiRadio index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Nom</th>
				<th>Ville</th>
				<th>fluxAudio</th>
				<th>fluxTxt</th>
				<th>codePostal</th>
				<th>coordonnees</th>
				<th>rayon</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for api_radio in api_radios %}
				<tr>
					<td>{{ api_radio.id }}</td>
					<td>{{ api_radio.nom }}</td>
					<td>{{ api_radio.ville }}</td>
					<td>{{ api_radio.fluxAudio }}</td>
					<td>{{ api_radio.fluxTxt }}</td>
					<td>{{ api_radio.codePostal }}</td>
					<td>{{ api_radio.coordonnees }}</td>
					<td>{{ api_radio.rayon }}</td>
					<td>
						<a href="{{ path('app_api_radio_edit', {'id': api_radio.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="6">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_api_radio_new') }}">Create new</a>
{% endblock %}
